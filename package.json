{
  "name": "oip-react",
  "version": "1.5.2",
  "description": "Simple React components written using Bootstrap 4 to allow easy use of the OIP stack anywhere!",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "npm run storybook",
    "start:prod": "npm run build && node config/production_server.js",
    "build": "webpack --config config/webpack.prod.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "docs:start": "npx styleguidist server",
    "docs:build": "npx styleguidist build",
    "compile": "npx babel src --out-dir lib",
    "prepare": "npm run compile"
  },
